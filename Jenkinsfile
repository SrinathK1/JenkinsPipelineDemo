pipeline {
    agent any

    stages {
        stage('start') {
            steps {
                echo 'Started the Build'
            }
        }
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploying') {
            steps {
                echo 'Deploying'
            }
        }
        stage('testing') {
            steps {
                echo 'testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Release'
            }
        }
    }
}
